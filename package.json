{
  "name": "jsoneditor-standalone",
  "version": "1.0.4",
  "homepage": "https://mishra-ankit.github.io/jsoneditor-standalone",
  "description": "This is a wrapper for jsoneditor package, it can be run offline without any dependencies and privacy concerns.",
  "dependencies": {
    "express": "4.17.1",
    "get-port": "5.1.0",
    "jsoneditor": "8.1.1",
    "lodash": "^4.17.15",
    "open": "6.0.0",
    "path": "0.12.7",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "devDependencies": {
    "react-scripts": "3.3.0"
  },
  "bin": {
    "jsoneditor": "bin/script.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mishra-ankit/jsoneditor-standalone.git"
  },
  "keywords": [
    "json",
    "editor",
    "formatter",
    "jsoneditor",
    "offline"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && rmdir /s /q docs && move build docs",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "author": "Ankit Mishra",
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
